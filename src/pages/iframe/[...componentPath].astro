---
import IframeHead from "@/components/IframeHead.astro";

const { componentPath } = Astro.params;

const components = await Astro.glob(`@/components/windstatic/**/*.astro`);

const component = components.find((c) => c.file.endsWith(componentPath));
---

<html lang="en" class="selection:text-blue selection:bg-blue/5">
  <head>
    <IframeHead />
  </head>
  <body>
    <component.default />
  </body>
</html>
