---
import Button from "@/components/fundations/buttons/Button.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import { AstroSeo } from "@astrolib/seo";
import Included from "@/components/global/Included.astro";
import Cta from "@/components/global/Cta.astro";
const pricingData = [
  {
    img: "/images/oxbow/windLogoDark.png",
    type: "Teams",
    price: "$199",
    futurePrice: "$399",
    audience: "Companies, teams & clients",
    features: [
      "Same as individual — plus get access to your team. This tier gives you access to up to 5 people.",
    ],
    cardStyles: "relative flex border p-8 bg-white  ",
    purchaseButton: {
      text: "Buy for",
      class:
        "inline-flex 2xl:text-base h-10 items-center focus:outline-none bg-white border border-base-300 focus:ring-2 focus:ring-black focus:ring-offset-2 font-medium hover:bg-base-50 px-4 py-1 rounded-full text-black text-sm duration-200 w-full text-center justify-center",
      link: "/api/buy/team",
    },
  },
  {
    img: "/images/oxbow/windLogo.png",
    type: "Individual",
    price: "$49 ",
    futurePrice: "$149",
    audience: "Personal and clients",
    features: [
      "All components ",
      "Lifetime access ",
      "Unlimited projects ",
      "Free updates ",
      "Use in SaaS",
    ],
    cardStyles: "relative flex border p-8 bg-base-50 order-first lg:order-none",
    purchaseButton: {
      text: "Buy for ",
      class:
        "text-white h-10 2xl:text-base bg-black focus:ring-2 focus:ring-white focus:ring-offset-2 inline-flex items-center px-4 py-1 rounded-full border border-transparent hover:bg-black/10 hover:text-black ring-1 ring-transparent duration-200 text-sm w-full justify-center",
      link: "/api/buy/individual",
    },
  },
  {
    img: "/images/oxbow/windLogo.png",
    type: "Demos",
    price: "$0",
    audience: "Every category has a free demo",
    features: ["1 Component from every category ready to copy to clipboard"],
    cardStyles: " relative flex border p-8 bg-white ",
    purchaseButton: {
      text: "Browse components",
      class:
        "inline-flex 2xl:text-base h-10 items-center focus:outline-none bg-white border border-base-300 focus:ring-2 focus:ring-black focus:ring-offset-2 font-medium hover:bg-base-50 px-4 py-1 rounded-full text-black text-sm duration-200 w-full text-center justify-center",
      link: "/",
    },
  },
];
---

<AstroSeo
  title="Oxbow UI - Pricing "
  description="Unlock permanent access to all components, elements and layouts and future additions for a one-time payment."
  canonical="https://www.oxbowui.com/pricing"
  openGraph={{
    url: "https://www.oxbowui.com/images/OpenGraph/twitter.png",
    title: "Oxbow UI - Pricing",
    description:
      "Unlock permanent access to all components, elements and layouts and future additions for a one-time payment.",
    images: [
      {
        url: "https://www.oxbowui.com/images/OpenGraph/facebook.png",
        width: 1200,
        height: 630,
        alt: "Oxbow UI",
        type: "image/png",
      },
      {
        url: "https://www.oxbowui.com/images/OpenGraph/twitter.png",
        width: 4096,
        height: 4096,
        alt: "Oxbow UI",
        type: "image/png",
      },
      { url: "https://www.oxbowui.com/images/OpenGraph/facebook.ong" },
      { url: "https://www.oxbowui.com/images/OpenGraph/twitter.png" },
    ],
    site_name: "Oxbow UI",
  }}
  twitter={{
    handle: "@Mike_Andreuzza",
    site: "https://www.oxbowui.com",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <section class="relative overflow-hidden">
    <div class="max-w-5xl 2xl:max-w-screen-xl px-8 mx-auto pt-12 relative">
      <div class="max-w-3xl 2xl:max-w-4xl mx-auto text-center">
        <div
          class="flex items-center gap-4 text-sm mx-auto text-base-500 justify-center">
          <div class="inline-flex gap-2 items-center">
            <svg
              class="flex-none inline size-6"
              viewBox="0 0 64 64"
              fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <g clip-path="url(#clip0_459_2)">
                <path
                  d="M32 12.75C23.4668 12.75 18.1332 17.0168 16 25.55C19.2 21.2833 22.9333 19.6832 27.2 20.75C29.6343 21.3585 31.3743 23.2498 33.3003 25.0795C36.4378 28.2643 40.0687 31.95 48 31.95C56.5332 31.95 61.8668 27.6832 64 19.15C60.8 23.4167 57.0667 25.0167 52.8 23.95C50.3657 23.3415 48.6257 21.4503 46.6997 19.6205C43.5622 16.4358 39.9313 12.75 32 12.75ZM16 31.95C7.46675 31.95 2.13325 36.2168 0 44.75C3.2 40.4833 6.93325 38.8833 11.2 39.95C13.6342 40.5585 15.3742 42.4497 17.3002 44.2795C20.4377 47.4642 24.0687 51.15 32 51.15C40.5332 51.15 45.8668 46.8833 48 38.35C44.8 42.6168 41.0667 44.2167 36.8 43.15C34.3657 42.5415 32.6258 40.6502 30.6998 38.8205C27.5623 35.6357 23.9313 31.95 16 31.95Z"
                  fill="#38BDF9"
                  stroke="#38BDF9"
                  stroke-width="0.25"
                ></path>
              </g>
              <defs>
                <clipPath id="clip0_459_2">
                  <rect
                    width="64"
                    height="64"
                    fill="white"
                  ></rect>
                </clipPath>
              </defs>
            </svg> Tailwind CSS
          </div>
          ·
          <div class="inline-flex gap-2 items-center">
            <svg
              class="flex-none inline size-6"
              viewBox="0 0 64 64"
              fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <g clip-path="url(#clip0_460_7)">
                <path
                  d="M49.7769 17L63.9992 31.16L49.7769 45.3197L35.5547 31.1597L49.7769 17Z"
                  fill="#77C1D2"
                ></path>
                <path
                  d="M14.2222 17L43.7067 46.355H15.2622L0 31.16L14.2222 17Z"
                  fill="#2D3441"
                ></path>
              </g>
              <defs>
                <clipPath id="clip0_460_7">
                  <rect
                    width="64"
                    height="29.5"
                    fill="white"
                    transform="translate(0 17)"
                  ></rect>
                </clipPath>
              </defs>
            </svg>
            Alpine JS
          </div>
        </div>
        <h1
          class="text-4xl mt-12 font-semibold tracking-tight text-base-900 lg:text-balance">
          One-time payment for lifetime access and future updates.
        </h1>
        <p class="text-balance mt-4 text-base-500 lg:text-balance 2xl:text-lg">
          Beta pricing available only during the beta phase
        </p>
        <p
          class="mt-8 inline-flex items-center rounded-full bg-accent-50 h-8 px-4 py-1 text-sm font-medium text-accent-600">
          No subscription required
        </p>
      </div>
      <div class=" xl:-mx-20">
        <div
          class="grid grid-cols-1 gap-4 sm:grid-cols-2 lg:grid-cols-3 mt-12 mx-auto">
          <!-- Tier 1 -->
          <div
            class="flex flex-col h-full justify-between p-1 bg-base-50 rounded-3xl shadow">
            <div>
              <div class="flex flex-col p-8 bg-white shadow rounded-[1.3rem]">
                <span class="text-xl text-base-900"> Teams </span>
                <p class="text-sm 2xl:text-lg text-base-500 mt-2">
                  For companies, teams, and clients, offering collaboration at
                  scale.
                </p>
              </div>
              <div class="p-8 pt-0 mt-8">
                <p
                  class="font-semibold lg:text-3xl flex items-baseline text-2xl tracking-tighter text-base-900">
                  $199 <del class="ml-2 opacity-50">$399</del>
                </p>
                <div class="w-full mt-4">
                  <a
                    id="purchase-teams-bundle"
                    data-umami-event="purchase-teams-bundle"
                    class="flex items-center justify-center transition-all duration-300 focus:ring-2 focus:ring-offset-2 focus:outline-none text-base-500 bg-white hover:text-accent-500 ring-1 ring-base-200 focus:ring-accent-500 focus:ring-offset-white h-9 px-4 py-2 text-sm font-medium rounded-md w-full"
                    href="/api/buy/team">
                    Buy for $199
                  </a>
                </div>
                <div class="pt-4 mt-4 border-t border-dashed border-base-200">
                  <p
                    class="text-xs 2xl:text-base text-base-500 font-mono uppercase tracking-wide">
                    Team plan includes:
                  </p>
                  <ul
                    class="flex flex-col text-sm 2xl:text-base mt-4 text-base-900 gap-y-3 font-medium"
                    role="list">
                    <li>
                      <div class="flex items-center gap-2">
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                          class="icon icon-tabler icons-tabler-outline icon-tabler-users-group size-4"
                          ><path
                            stroke="none"
                            d="M0 0h24v24H0z"
                            fill="none"
                          ></path><path d="M10 13a2 2 0 1 0 4 0a2 2 0 0 0 -4 0"
                          ></path><path
                            d="M8 21v-1a2 2 0 0 1 2 -2h4a2 2 0 0 1 2 2v1"
                          ></path><path d="M15 5a2 2 0 1 0 4 0a2 2 0 0 0 -4 0"
                          ></path><path d="M17 10h2a2 2 0 0 1 2 2v1"
                          ></path><path d="M5 5a2 2 0 1 0 4 0a2 2 0 0 0 -4 0"
                          ></path><path d="M3 13v-1a2 2 0 0 1 2 -2h2"
                          ></path></svg
                        >
                        <span>Get access to your team — Up to 5 people.</span>
                      </div>
                      <p class="text-sm text-base-500 mt-2 2xl:text-base">
                        Team licenses provide access for up to 5 members,
                        ensuring coverage for even the largest teams within your
                        organization.
                      </p>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
          <!-- Tier 2 -->
          <div
            class="flex h-full justify-between flex-col p-1 rounded-3xl bg-base-900 shadow">
            <div>
              <div
                class="flex flex-col p-8 bg-base-800 shadow rounded-[1.3rem]">
                <span class="text-xl text-white"> Individual </span>
                <p class="text-sm 2xl:text-lg text-base-300 mt-2">
                  Perfect for personal use, freelance and individual client
                  work.
                </p>
              </div>
              <div class="p-8 pt-0 mt-8">
                <p
                  class="font-semibold flex lg:text-3xl items-baseline text-2xl tracking-tighter text-white">
                  $49 <del class="ml-2 opacity-50">$149</del>
                </p>
                <div class="w-full mt-4">
                  <a
                    id="purchase-individual-bundle"
                    data-umami-event="purchase-individual-bundle"
                    class="flex items-center justify-center transition-all duration-300 focus:ring-2 focus:ring-offset-2 focus:outline-none text-white bg-accent-600 hover:bg-accent-700 ring-accent-700 focus:ring-accent-700 focus:ring-offset-white h-9 px-4 py-2 text-sm font-medium rounded-md w-full"
                    href="/api/buy/individual">
                    Buy for $49
                  </a>
                </div>
                <div class="pt-4 mt-4 border-t border-dashed border-base-700">
                  <p
                    class="text-xs 2xl:text-base text-base-200 font-mono uppercase tracking-wide">
                    Individual plan includes:
                  </p>
                  <ul
                    class="flex flex-col mt-4 2xl:text-base text-sm text-white gap-y-6 font-medium"
                    role="list">
                    <li>
                      <div class="flex items-center gap-2">
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                          class="icon icon-tabler icons-tabler-outline icon-tabler-infinity size-4"
                          ><path
                            stroke="none"
                            d="M0 0h24v24H0z"
                            fill="none"
                          ></path><path
                            d="M9.828 9.172a4 4 0 1 0 0 5.656a10 10 0 0 0 2.172 -2.828a10 10 0 0 1 2.172 -2.828a4 4 0 1 1 0 5.656a10 10 0 0 1 -2.172 -2.828a10 10 0 0 0 -2.172 -2.828"
                          ></path></svg
                        >
                        <span>Lifetime access </span>
                      </div>
                      <p class="text-sm text-base-300 mt-2">
                        Gain immediate access to all our current offerings,
                        along with any new components and templates we release
                        in the future.
                      </p>
                    </li>

                    <li>
                      <div class="flex items-center gap-2">
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                          class="icon icon-tabler icons-tabler-outline icon-tabler-components size-4"
                          ><path
                            stroke="none"
                            d="M0 0h24v24H0z"
                            fill="none"
                          ></path><path d="M3 12l3 3l3 -3l-3 -3z"></path><path
                            d="M15 12l3 3l3 -3l-3 -3z"></path><path
                            d="M9 6l3 3l3 -3l-3 -3z"></path><path
                            d="M9 18l3 3l3 -3l-3 -3z"></path></svg
                        >
                        <span>All components </span>
                      </div>
                      <p class="text-sm text-base-300 mt-2">
                        All the tools you need to create stunning application
                        interfaces, marketing websites, online stores, and
                        beyond.
                      </p>
                    </li>
                    <li>
                      <div class="flex items-center gap-2">
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                          class="icon icon-tabler icons-tabler-outline icon-tabler-tools size-4"
                          ><path
                            stroke="none"
                            d="M0 0h24v24H0z"
                            fill="none"
                          ></path><path
                            d="M3 21h4l13 -13a1.5 1.5 0 0 0 -4 -4l-13 13v4"
                          ></path><path d="M14.5 5.5l4 4"></path><path
                            d="M12 8l-5 -5l-4 4l5 5"></path><path
                            d="M7 8l-1.5 1.5"></path><path
                            d="M16 12l5 5l-4 4l-5 -5"></path><path
                            d="M16 17l-1.5 1.5"></path></svg
                        >
                        <span>Unlimited projects </span>
                      </div>
                      <p class="text-sm text-base-300 mt-2">
                        Use our components and templates across as many projects
                        as you need, with no limits or restrictions.
                      </p>
                    </li>
                    <li>
                      <div class="flex items-center gap-2">
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                          class="icon icon-tabler icons-tabler-outline icon-tabler-refresh size-4"
                          ><path
                            stroke="none"
                            d="M0 0h24v24H0z"
                            fill="none"
                          ></path><path
                            d="M20 11a8.1 8.1 0 0 0 -15.5 -2m-.5 -4v4h4"
                          ></path><path
                            d="M4 13a8.1 8.1 0 0 0 15.5 2m.5 4v-4h-4"
                          ></path></svg
                        >
                        <span>Free updates</span>
                      </div>
                      <p class="text-sm text-base-300 mt-2">
                        Stay ahead with regular updates, gaining access to the
                        latest components and improvements at no extra cost.
                      </p>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
          <!-- Tier 3 -->
          <div
            class="flex flex-col h-full justify-between p-1 bg-base-50 rounded-3xl shadow">
            <div>
              <div class="flex flex-col p-8 bg-white shadow rounded-[1.3rem]">
                <span class="text-xl text-base-900"> Demos </span>
                <p class="text-sm 2xl:text-lg text-base-500 mt-2">
                  Explore free demos for every category—copy, paste, and see how
                  they work.
                </p>
              </div>
              <div class="p-8 pt-0 mt-8">
                <p
                  class="font-semibold lg:text-3xl flex items-baseline text-2xl tracking-tighter text-base-900">
                  $0
                </p>
                <div class="w-full mt-4">
                  <a
                    id="purchase-demos-bundle"
                    data-umami-event="purchase-demos-bundle"
                    class="flex items-center justify-center transition-all duration-300 focus:ring-2 focus:ring-offset-2 focus:outline-none text-base-500 bg-white hover:text-accent-500 ring-1 ring-base-200 focus:ring-accent-500 focus:ring-offset-white h-9 px-4 py-2 text-sm font-medium rounded-md w-full"
                    href="/">
                    Browse components $0
                  </a>
                </div>
                <div class="pt-4 mt-4 border-t border-dashed border-base-200">
                  <p
                    class="text-xs 2xl:text-base text-base-500 font-mono uppercase tracking-wide">
                    Demos include:
                  </p>
                  <ul
                    class="flex flex-col text-sm 2xl:text-base mt-4 text-base-900 gap-y-3 font-medium"
                    role="list">
                    <li>
                      <div class="flex items-center gap-2">
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="24"
                          height="24"
                          viewBox="0 0 24 24"
                          fill="none"
                          stroke="currentColor"
                          stroke-width="2"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                          class="icon icon-tabler icons-tabler-outline icon-tabler-plus size-4"
                          ><path
                            stroke="none"
                            d="M0 0h24v24H0z"
                            fill="none"
                          ></path><path d="M12 5l0 14"></path><path
                            d="M5 12l14 0"></path></svg
                        >
                        <span>All demo components </span>
                      </div>
                      <p class="text-sm text-base-500 mt-2 2xl:text-base">
                        One component from each category is available for easy
                        copying to your clipboard without the need for a
                        subscription or registration.
                      </p>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div
        class="mt-10 grid grid-cols-1 gap-1 md:grid-cols-3 text-left sr-only">
        {
          pricingData.map((tier) => (
            <div
              class={`relative flex flex-col size-full  justify-between p-8 2xl:p-16 gap-8   ${tier.cardStyles}`}>
              <ul
                class="flex flex-col list-disc list-inside"
                role="list">
                <li class="block font-medium text-black 2xl:text-2xl">
                  {tier.type} —{" "}
                  <>
                    <span>{tier.price}</span>
                    <del class="opacity-50">{tier.futurePrice}</del>
                  </>
                </li>
                <li class="block text-sm 2xl:text-lg text-base-500  pb-3 mt-4">
                  {tier.audience}
                </li>
                {tier.features.map((feature, i) => {
                  const [startText, ...rest] = feature.split(" — ");
                  const restText = rest.join(" — ");
                  return (
                    <li class="text-sm mt-3 2xl:text-lg font-medium text-base-900">
                      {startText}
                      {restText && ` — ${restText}`}
                    </li>
                  );
                })}
              </ul>
              <div>
                {tier.purchaseButton && (
                  <a
                    id={`purchase-${tier.type.toLowerCase().replace(/\s/g, "-")}-bundle`}
                    data-umami-event={`purchase-${tier.type.toLowerCase().replace(/\s/g, "-")}-bundle`}
                    class={tier.purchaseButton.class}
                    href={tier.purchaseButton.link}>
                    {tier.purchaseButton.text} {tier.price}
                  </a>
                )}
              </div>
            </div>
          ))
        }
      </div>

      <div class="grid grid-cols-1 mt-5 md:grid-cols-2 items-center w-full">
        <div class="inline-flex items-center gap-3">
          <h1 class="text-netral-500">Secure Payment via</h1>
          <img
            class="h-5"
            src="/images/oxbow/lemon.svg"
            alt=""
          />
        </div>
        <div class="mt-3 md:mt-0 lg:ml-auto">
          <ul
            class="flex flex-wrap list-none"
            role="list">
            <li>
              <img
                height="50"
                src="/images/oxbow/paypal.svg"
                alt=""
              />
            </li>
            <li>
              <img
                height="50"
                src="/images/oxbow/applepay.svg"
                alt=""
              />
            </li>
            <li>
              <img
                height="50"
                src="/images/oxbow/googlepay.svg"
                alt=""
              />
            </li>
            <li>
              <img
                height="50"
                src="/images/oxbow/mastercard.svg"
                alt=""
              />
            </li>
            <li>
              <img
                height="50"
                src="/images/oxbow/visa.svg"
                alt=""
              />
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>
  <Included />
  <Cta />
</BaseLayout>
