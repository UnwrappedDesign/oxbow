---
import ScreenLayout from "@/layouts/ScreenLayout.astro";
---

<ScreenLayout>
  <section
    class="relative overflow-hidden"
    x-data="{duration: 'monthly'}">
    <div
      class="absolute inset-x-0 top-0 h-96 rotate-180 text-gray-400/30 opacity-60 [mask-image:linear-gradient(to_bottom,transparent,white)]">
      <svg
        class="absolute inset-0 h-full w-full"
        xmlns="http://www.w3.org/2000/svg"
        ><defs
          ><pattern
            id="grid-pattern"
            width="32"
            height="32"
            patternUnits="userSpaceOnUse"
            x="50%"
            y="100%"
            patternTransform="translate(0 -1)"
            ><path
              d="M0 32V.5H32"
              fill="none"
              stroke="currentColor"
            ></path></pattern
          ></defs
        ><rect
          width="100%"
          height="100%"
          fill="url(#grid-pattern)"
        ></rect></svg
      >
    </div>

    <div class="px-8 py-24 mx-auto md:px-12 lg:px-32 max-w-screen-xl relative">
      <!---- Use only from lg: -->
      <div class="hidden mt-24 lg:block">
        <table class="w-full mx-auto text-center bg-transparent">
          <thead>
            <tr class="align-top">
              <th
                class="w-1/3  py-4 bg-transparent font-normal text-left align-bottom    ">
                <div
                  class="w-full border bg-white overflow-hidden inline-flex mt-8 p-0.5 rounded-lg max-w-xs shadow shadow-white/20 z-0">
                  <!-- Monthly Pricing Button: Sets 'duration' to 'monthly' when clicked -->
                  <button
                    id="monthly-pricing-button"
                    class="block w-full px-8 py-2 text-xs font-medium transition border border-transparent rounded-md text-blue-900"
                    @click="duration = 'monthly'"
                    :class="duration == 'monthly' ? 'bg-gray-100 border text-blue-900 ' : ''"
                    type="button">
                    Monthly
                  </button>

                  <!-- Annual Pricing Button: Sets 'duration' to 'annual' when clicked -->
                  <button
                    id="annual-pricing-button"
                    class="block w-full px-8 py-2 text-xs font-medium transition border border-transparent rounded-md text-blue-900"
                    @click="duration = 'annual'"
                    :class="duration == 'annual' ? 'bg-gray-100 border text-blue-900 ' : ''"
                    type="button">
                    Annual
                  </button>
                </div>
              </th>
              <th
                class="w-1/3 px-2 py-4 bg-transparent font-normal text-left relative">

                <div
                  class="font-mono text-base font-medium tracking-tighter uppercase text-blue-950">
                  Hobby
                </div>
                <p
                  class="font-semibold order-first text-xl tracking-tighter font-mono mt-8 text-blue-950">
                  <span
                    data-monthly="$9"
                    data-annual="$5"
                    x-text="$el.dataset[duration]"
                  ></span>
                  <span
                    class="text-sm font-normal tracking-normal text-gray-700">
                    <span x-show="duration === 'monthly'">
                      <span x-show="duration === 'monthly'"> /month</span></span
                    >
                    <span
                      x-show="duration === 'annual'"
                      style="display: none;">
                      /annually</span
                    >
                  </span>
                </p>
                <p class="text-xs text-gray-700 mt-4">
                  This subscription tier caters to individuals and hobbyists
                  seeking fundamental features.
                </p>
                <div class="mt-8">
                  <button
                    type="button"
                    aria-label="Get started with plan-name"
                    class="flex items-center justify-center gap-2 h-10 px-4 py-2 text-sm font-semibold text-gray-500 transition-all duration-200 bg-gray-50 border border-gray-300 rounded-lg hover:text-blue-700 focus:ring-2 shadow shadow-gray-500/5 focus:ring-blue-950 focus:ring-offset-2 ring-offset-gray-200 hover:shadow-none w-full"
                    >Get started</button
                  >
                </div>
              </th>
              <th
                class="w-1/3 px-2 py-4 bg-transparent font-normal text-left ">
                <div
                  class="font-mono text-base flex items-center justify-between font-medium tracking-tighter uppercase text-blue-950">
                  GOLDEN UNICORN
                  <span
                    class="inline-flex shadow items-center gap-1 px-2 py-1 text-xs font-medium text-white bg-blue-500 rounded-md ring-1 ring-inset ring-gray-500/10"
                    >Popular
                </div>
                <p
                  class="font-semibold order-first text-xl tracking-tighter mt-8 font-mono text-blue-950">
                  <span
