---
const images = [
  "/thumbs/1.svg",
  "/thumbs/2.svg",
  "/thumbs/3.svg",
  "/thumbs/4.svg",
  "/thumbs/5.svg",
  "https://images.unsplash.com/photo-1502921935-be74c0dc50fd?q=80&w=2670&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D",
];
---

<section>
  <div
    class="px-8 py-24 grid mx-auto md:px-12 lg:px-32 max-w-7xl grid-cols-1 gap-2 sm:grid-cols-2 md:grid-cols-3"
  >
    {
      images.map((image, index) => (
        <img
          class="aspect-square object-cover object-center"
          src={image}
          alt={`${index + 1}_`}
        />
      ))
    }
  </div>
</section>
